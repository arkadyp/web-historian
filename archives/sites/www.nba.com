			<script language="JavaScript">
				TargetDate		= "11/9/2013 11:59:59 PM UTC-0500";
				BackColor 		= "none";
				ForeColor		= "#FFF";
				CountActive 	= true;
				CountStepper	= -1;
				LeadingZero		= true;
				DisplayFormat	= "%%D%% %%H%% %%M%% %%S%%";
				FinishMessage	= "Now Playing&nbsp;";

				function countKia(secs) {
					var kia = $("#cntdwn_Kia");
					/* is the countdown complete? */
					if (secs < 0) {
						setTimeout(deleteKia, 100);
						return;
					}
					/* while we're counting down to midnight, show the now playing kia banner to the right of the searchbar */
					$( '#body_Right_Kia' ).addClass( 'now_playing_kia_right' );
					$(".nbaSkin").addClass("kia");
					$(".nbaSkin").addClass("bigIsOnRed");

					if (CountActive) {
						setTimeout("countKia(" + (secs + CountStepper) + ")", SetTimeOutPeriod);
					}
				}
				
				function deleteKia(){
					/* remove the Kia Countdown Timer and replace the BG Skin */
					$('#body_Right_Kia').remove();
					/* remove the BIG IS ON red class */
					$(".nbaSkin").removeClass("bigIsOnRed");
					/* add the BIG IS ON class */
					$(".nbaSkin").addClass("bigIsOn");
					/* add in the TNT OT clock */
					$('#body_Right_Tnt').load('/.element/ssi/sect/3.0/home/countdownClockAuto.html'); 
				}				
				
				function createKia() {
					var dthen = new Date(TargetDate);
					var dnow = new Date();

					if ( CountStepper > 0 ) {
						ddiff = new Date(dnow - dthen);
					} else {
						ddiff = new Date(dthen - dnow);
					}

					/* determine how many seconds are left in the countdown */
					gsecs = Math.floor( ddiff.valueOf() / 1000 );
					/* executed upon page load */
					if( gsecs < 0 ) {
						/* remove the Kia Countdown Timer and replace the BG Skin */
						$('#body_Right_Kia').remove();
						/* remove the BIG IS ON red class */
						$(".nbaSkin").removeClass("bigIsOnRed");
						//Inputting ASB promo
						//$(".nbaSkin").html("<div class='nbaASB'><a href='http://allstarballot.nba.com/ballot?referrer=asb14_searchbar&cid=asb14_searchbar' target='_blank'><img src='http://i.cdn.turner.com/nba/nba/.element/img/3.0/sect/home/ASB_domestic.png' width='260' height='40'/><\/a><\/div>");
						/* add the KIA & BIG IS ON class */
						//$(".nbaSkin").addClass("kia");
						//$(".nbaSkin").addClass("bigIsOn");
						/* add in the TNT OT clock */
						$('#body_Right_Tnt').load('/.element/ssi/sect/3.0/home/countdownClockAuto.html'); 
					} else {
						countKia(gsecs);
					}
				}
				
				CountStepper = Math.ceil( CountStepper );

				if ( CountStepper == 0 ) { CountActive = false; }

				var SetTimeOutPeriod = ( Math.abs( CountStepper ) - 1 ) * 1000 + 990;
				
				$( document ).ready( function() {
					/* create the HTML */
					setTimeout( createKia, 100 );
				});
			</script>
		
    

